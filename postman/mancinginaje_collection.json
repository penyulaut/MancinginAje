            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Extract CSRF token from cookie and save to environment",
                  "var xsrfCookie = pm.cookies.get('XSRF-TOKEN');",
                  "if (xsrfCookie) {",
                  "    pm.environment.set('csrf_token', decodeURIComponent(xsrfCookie));",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
